{
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "quotemark": [true, "single"],
    "indent": [true, "spaces", 2],
    "max-line-length": [true, 100],
    "interface-name": [true, "never-prefix"],
    "no-implicit-dependencies": [true, "dev"],
    "max-classes-per-file": false,
    "object-literal-sort-keys": false
  },
  "jsRules": true
}
